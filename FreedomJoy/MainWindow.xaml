<Window x:Class="FreedomJoy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FreedomJoy"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400"
        d:DataContext="{d:DesignInstance local:MainWindow}">
    <Grid>
        <Button x:Name="ButtonRunMain" Content="{Binding RunMainButtonText}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="ButtonRunMain_Click" Height="20"/>
        <Ellipse x:Name="MainIndicator"  Fill="{Binding IndicatorFill}" HorizontalAlignment="Left" Height="20" Margin="363,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="19"/>
    </Grid>
</Window>
